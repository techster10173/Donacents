{
  "name": "Dona-cents",
  "version": "1.0",
  "description": "Collect spare cents and donate them to your chosen charity",
  "manifest_version": 2,
  "browser_action":{
    "default_popup": "index.html"
  },
  "background": {
    "scripts":["main.js","getPagesSource.js"]
  },
  "permissions": [
    "https://*/",
    "tabs",
    "storage",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://kit.fontawesome.com/; object-src 'self'",
  "icons": {
    "128": "LogoMakr_1F7kTi.png"
  }
}